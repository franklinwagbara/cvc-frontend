{
  "name": "CVCFrontEnd",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "clean:cvcfrontend-sdk": "rimraf *.tgz ./sdk/cvcfrontend-sdk",
    "generate-core-api-sdk:local": "npm run clean:cvcfrontend-sdk && openapi-generator-cli generate -g typescript-angular -i http://localhost:5000/v3/api-docs -o ./sdk/cvcfrontend-sdk",
    "generate-core-api-sdk:staging": "npm run clean:cvcfrontend-sdk && openapi-generator-cli generate -g typescript-angular -i https://cvc-dev.azurewebsites.net/v3/api-docs -o ./sdk/cvcfrontend-sdk",
    "generate-core-api-sdk:prod": "npm run clean:cvcfrontend-sdk && openapi-generator-cli generate -g typescript-angular -i https://cvc.azurewebsites.net/v3/api-docs -o ./sdk/cvcfrontend-sdk"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.29",
    "@angular/animations": "~14.2.6",
    "@angular/cdk": "^14.2.5",
    "@angular/common": "~14.2.6",
    "@angular/compiler": "~14.2.6",
    "@angular/core": "~14.2.6",
    "@angular/forms": "~14.2.6",
    "@angular/material": "^14.2.5",
    "@angular/platform-browser": "~14.2.6",
    "@angular/platform-browser-dynamic": "~14.2.6",
    "@angular/router": "~14.2.6",
    "@auth0/angular-jwt": "^5.1.0",
    "@flaticon/flaticon-uicons": "^3.0.0",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "jsonwebtoken": "^9.0.1",
    "ng-multiselect-dropdown": "^1.0.0-beta.19",
    "ngx-material-timepicker": "^13.1.1",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "util": "^0.12.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "~14.2.6",
    "@angular/compiler-cli": "~14.2.6",
    "@angular/localize": "^14.1.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.28.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
